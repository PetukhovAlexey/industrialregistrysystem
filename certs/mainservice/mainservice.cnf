[ req ] 
default_bits = 4096 
prompt = no 
default_md = sha256 
distinguished_name = dn 
req_extensions = req_ext 
 
[ dn ] 
C = RU 
ST = Moscow 
L = Moscow 
O = IndustrialRegistrySystem 
OU = MainService 
CN = mainservice 
 
[ req_ext ] 
subjectAltName = @alt_names 
keyUsage = digitalSignature, keyEncipherment 
extendedKeyUsage = serverAuth, clientAuth 
 
[ alt_names ] 
DNS.1 = mainservice 
DNS.2 = localhost 
IP.1 = 127.0.0.1 
